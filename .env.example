# 服务端口 (使用 3210 避免常见端口冲突)
PORT=3210

# 浏览器模式 (true=无头, false=可视化)
HEADLESS=false

# 用户数据目录
USER_DATA_DIR=storage/user_data

# 视口尺寸 (默认 1920x1080 Full HD)
VIEWPORT_WIDTH=1920
VIEWPORT_HEIGHT=1080

# 心跳间隔 (毫秒)
HEARTBEAT_INTERVAL=30000

# Chrome DevTools (true=自动打开DevTools, false=关闭)
# 启用后会自动打开 DevTools 并开启远程调试端口 9222
DEVTOOLS=true

# 操作延迟 (毫秒, 0=最快速度)
# 设为 0 以获得最佳性能
SLOW_MO=0
